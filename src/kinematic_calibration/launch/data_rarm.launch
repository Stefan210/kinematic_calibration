
<launch>
  <rosparam file="$(find kinematic_calibration)/config/params_rarm_general.yaml" command="load"/>
  <node pkg="kinematic_calibration" type="dataCaptureMain" name="DataCapture" output="screen" >
    <rosparam file="$(find kinematic_calibration)/config/params_rarm_capturing.yaml" command="load" ns="params" />
  </node>
  <node pkg="nao_remote" type="pose_manager.py" name="pose_manager">
    <rosparam file="$(find kinematic_calibration)/config/poses_rarm.yaml" command="load" ns="poses" />
  </node>  
</launch>
